// SublimeLinter Settings - User
{
    "paths": {
        "linux": ["~/miniconda3/envs/linters/bin/"],
        "windows": ["f:/miniconda3/envs/linters/Scripts"],
    },
    "linters": {
        "pylint": {
            "rcfile": "~/.pylintrc"
        },
        "mypy": {
            "args": [
                "--ignore-missing-imports",
                "--disallow-untyped-defs",
                "--disallow-untyped-calls",
                "--disallow-incomplete-defs",
            ]
        }
    }
}
